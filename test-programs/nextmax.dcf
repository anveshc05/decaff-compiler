class Program {
    int arr[50];
    int main() {
        int max = -1000000005;
        int next_max = -1000000005;
        callout("scanf","%d\n", &n);
        if( n < 2){
            callout("printf", "Invalid Input");
            return 0;            
        }
        int i = 0;
        for i = 0, n {
            arr[i] = callout("scanf", "%d", &arr[i]);
            if( arr[i] > max){
                next_max = max;
                max = arr[i];
            }
            else{
                if(arr[i] < next_max){
                    next_max = arr[i];
                }
            }
        }
        callout("printf", "Second Largest Element in the array = %d\n", next_max);
        return 0;
    }
}
